{
  "compilerOptions": {
    "outDir": "build/es5",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "declaration": true
  },
  "exclude": ["build", "node_modules"]
}
